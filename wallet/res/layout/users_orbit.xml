<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="483dp"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:clipToPadding="false">

    <com.sambarboza.orbitview.OrbitView
        android:layout_width="175dp"
        android:layout_height="175dp"
        android:layout_centerInParent="true"
        app:orbitColor="#80008DE4"
        app:orbitWidth="1dp">

        <com.sambarboza.orbitview.PlanetView
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:clipToPadding="false"
            app:animStartDelay="600"
            app:drawable="@drawable/user1"
            app:fromAngle="70"
            app:interpolator="decelerate"
            app:orbitDuration="300"
            app:toAngle="135" />

    </com.sambarboza.orbitview.OrbitView>

    <com.sambarboza.orbitview.OrbitView
        android:layout_width="270dp"
        android:layout_height="270dp"
        android:layout_centerInParent="true"
        app:orbitColor="#4D008DE4"
        app:orbitWidth="1dp">

        <com.sambarboza.orbitview.PlanetView
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:clipToPadding="false"
            app:animStartDelay="600"
            app:drawable="@drawable/user2"
            app:fromAngle="55"
            app:interpolator="decelerate"
            app:orbitDuration="350"
            app:toAngle="110" />

    </com.sambarboza.orbitview.OrbitView>

    <com.sambarboza.orbitview.OrbitView
        android:layout_width="368dp"
        android:layout_height="368dp"
        android:layout_centerInParent="true"
        app:orbitColor="#1A008DE4"
        app:orbitWidth="1dp">

        <com.sambarboza.orbitview.PlanetView
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:clipToPadding="false"
            app:animStartDelay="600"
            app:drawable="@drawable/user3"
            app:fromAngle="65"
            app:interpolator="decelerate"
            app:orbitDuration="400"
            app:toAngle="130" />

    </com.sambarboza.orbitview.OrbitView>

    <com.sambarboza.orbitview.OrbitView
        android:layout_width="483dp"
        android:layout_height="483dp"
        android:layout_centerInParent="true"
        app:orbitColor="#12008DE4"
        app:orbitWidth="1dp">

        <com.sambarboza.orbitview.PlanetView
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:clipToPadding="false"
            app:animStartDelay="600"
            app:drawable="@drawable/user4"
            app:fromAngle="80"
            app:interpolator="decelerate"
            app:orbitDuration="450"
            app:toAngle="160" />

        <com.sambarboza.orbitview.PlanetView
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:clipToPadding="false"
            app:animStartDelay="600"
            app:drawable="@drawable/user5"
            app:duration="500"
            app:fromAngle="50"
            app:interpolator="decelerate"
            app:toAngle="110" />

    </com.sambarboza.orbitview.OrbitView>

</RelativeLayout>

